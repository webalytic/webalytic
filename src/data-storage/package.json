  "dependencies": {
    "@webalytic/ms-tools": "^0.0.12",
    "@webalytic/protorepo": "webalytic/protorepo#0.2.6",
    "@cubejs-backend/server-core": "^0.19.56",
    "@cubejs-backend/clickhouse-driver": "^0.19.56",
    "@types/protobufjs": "^6.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "faker": "^4.1.0",
    "moment": "^2.27.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.6",
    "umzug": "^2.3.0",
    "protobufjs": "^6.10.1",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4"
  },
  "devDependencies": {
    "@types/faker": "^4.1.12",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-es5": "^1.5.0",
    "eslint-plugin-import": "^2.22.0",
    "grpc-tools": "^1.9.1",
    "grpc_tools_node_protoc_ts": "^4.1.2"
  },
  "scripts": {
    "start": "ts-node --transpile-only -r tsconfig-paths/register --project ./tsconfig.json ./src/index.ts",
    "build": "ts-node --transpile-only -r tsconfig-paths/register --project ./tsconfig.json ./bin/proto.ts",
    "generator": "ts-node --transpile-only -r tsconfig-paths/register --project ./tsconfig.json ./src/generator.ts",
    "lint": "eslint --ext .js,.ts ./",
    "ts-check": "tsc -p ./tsconfig.json --noEmit",
    "migration": "ts-node --transpile-only -r tsconfig-paths/register --project ./tsconfig.json ./migrations",
    "migration:down": "ts-node --transpile-only -r tsconfig-paths/register --project ./tsconfig.json ./migrations down",
    "postinstall": "npm run build"
  }
}
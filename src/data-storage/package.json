    "lint": "eslint --ext .js,.ts ./",
    "ts-check": "tsc -p ./tsconfig.json --noEmit",
    "migration": "ts-node --transpile-only -r tsconfig-paths/register --project ./tsconfig.json ./migrations",
    "migration:down": "ts-node --transpile-only -r tsconfig-paths/register --project ./tsconfig.json ./migrations down"
